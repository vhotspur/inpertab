#, php-format
msgid "Attempts needed: <span></span>."
msgstr "Neúspěšných pokusů: <span></span>."

#. l10n:POSITION: 1-3-4-1-1
#: /dev/stdin:22
#, php-format
msgid "It took you <span id='p-pinpointgame-stats-total'></span> tries to find <span id='p-pinpointgame-stats-elements'></span> elements."
msgstr "Potřebovali jste <span id='p-pinpointgame-stats-total'></span> pokusů abyste našli <span id='p-pinpointgame-stats-elements'></span> prvků."

#. l10n:POSITION: 1-3-4-2-1
#: /dev/stdin:25
#, php-format
msgid "You scored <span id='p-pinpointgame-stats-first-count'></span> on the first hit."
msgstr "<span id='p-pinpointgame-stats-first-count'></span>× jste se trefili na poprvé."

