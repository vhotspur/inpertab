#, php-format
msgid "Element pin-pointing"
msgstr "Hra: nalezení prvku"

#. l10n:POSITION: 1-3-2-1
#: /dev/stdin:10
#, php-format
msgid "Pin-point the following element: <span id='p-pinpointgame-clue'></span>."
msgstr "Najděte následující prvek: <span id='p-pinpointgame-clue'></span>."

#. l10n:POSITION: 1-3-3-1-1-1
#: /dev/stdin:13
#, php-format
msgid "Congratulations! You got it right :-)."
msgstr "Výborně! Nalezen :-)."

#. l10n:POSITION: 1-3-3-1-3-1
